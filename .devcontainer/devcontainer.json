{
  "name": "Taskifyappdesign Dev Container",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:18",
  "forwardPorts": [5173],
  "postCreateCommand": "npm ci && npm rebuild esbuild --update-binary",
  "remoteUser": "node",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
